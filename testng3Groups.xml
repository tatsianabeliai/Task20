<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="smokeAndFast">
    <test name="groups">
        <groups>
            <run>
                <include name="smoke"/>
                <include name="fast"/>
                <exclude name="smoke"/>
            </run>
        </groups>
        <classes>
            <class name="ClassOneTest"/>
        </classes>
    </test>
</suite>
